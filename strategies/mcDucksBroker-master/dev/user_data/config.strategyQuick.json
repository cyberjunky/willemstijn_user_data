{
  "strategy": "StrategyScalpingFast2",
  "max_open_trades": 20,
  "stake_amount": "unlimited",
  "tradable_balance_ratio": 0.99,
  "timeframe": "1m",
  "dry_run": true,
  "cancel_open_orders_on_exit": true,
  "amend_last_stake_amount": true,  
  "unfilledtimeout": {
    "buy": 10,
    "sell": 30
  },
  "stoploss_on_exchange": true,
  "bid_strategy": {
    "use_order_book": true,
    "ask_last_balance": 1.0,
    "order_book_top": 1,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },
  "ask_strategy": {
    "use_order_book": false,
    "order_book_min": 1,
    "order_book_max": 9
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ]
}